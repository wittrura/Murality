<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="https://unpkg.com/purecss@1.0.0/build/pure-min.css" integrity="sha384-nn4HPE8lTHyVtfCBi5yW9d20FjT8BJwUXyWZT9InLYax14RDjBj46LmSztkmNP9w" crossorigin="anonymous">
    <meta charset="utf-8">
    <title>Tours</title>
  </head>
  <body>
    <h1>Tour List</h1>
    <% let curTourId = 0; %>
    <% let tourCount = 1; %>
    <% let firstList = true; %>
      <% toursMurals.forEach(tour => { %>
        <% if (tour.tour_id !== curTourId) { %>
          <% if (firstList) { %>
            <% firstList = false; %>
          <% } else { %>
          </ol>
          <% } %>
          <h3><%= tourCount++ %></h3>
          <ol>
        <% } %>
            <li><%= tour.muralName + ', ' + tour.artistName %></li>
      <% }) %>
    </ol>
  </body>
</html>
